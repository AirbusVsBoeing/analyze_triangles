package triangle.analyze;

import java.util.List;

public class Triangle {
	
	private Graph graph;
	
	public List<Graph> listTriangles(){
		
		for(int i = 0; i < graph.size(); i++) {
			for(int j = 0; j < graph.size(); j++) {
				for(int k = 0; k < graph.size(); k++) {
					if(i!=j && j!=k && k!=i) {
						if isTriangle()
					}
				}
			}
		}
			
		
		return null;
	}
	
	
}
